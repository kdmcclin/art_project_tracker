<p id="notice"><%= notice %></p>

<h1>Projects Lost to the Void</h1>
<h3>Good luck finding your art project again!</h3>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= @projects.count %>
    <% @projects.each do |project| %>
      <tr>
        <td><strong>Date:</strong><br><%= project.created_at.strftime("%m/%d/%Y") %></td>
        <td><strong>Tracking number:</strong> <br><%= SecureRandom.base64%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path, id: "new-project" %>